int	p(char *dest, char *string)
{
	char c[0x1000];
	int i;

	puts(string);
	read(0, c, 0x1000);
	i = strchr(c, '\n');
	c[i] = 0;
	strncpy(dest, c, 20);

}

int	pp(char *dest)
{
	char s1[20];
	char s2[20];
	size_t i;
	
	p(s1, " - ");
	p(s2, " - ");
	strcpy(dest,s1);
	i = strlen(dest);
	sn[i];
	strcat(dest, s2);
	return();
}

int main()
{
	char dest[20];

	pp(dest);
	puts(dest);
	return();

}
